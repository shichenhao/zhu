.orderBox {
  min-height: 100vh;
  background: #eee;
  padding: 0px 20px;
}
.orderBox .result {
  position: absolute;
  padding-left: 10px;
  bottom: 0;
  left: 0;
  right: 0;
  height: 50px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 16px;
  background: #ffffff;
}
.orderBox .result .submit {
  padding: 0 20px;
}
.orderBox .price {
  color: #FFA200;
}
.orderBox .ulTitle {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  background: #eeeeee;
  line-height: 40px;
  padding: 0 10px;
  font-size: 16px;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.orderBox .ulTitle image {
  width: 20px;
  height: auto;
  margin-right: 5px;
}
.orderBox .ulTitle:after {
  content: ' ';
  display: block;
  margin-left: 5px;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  border-bottom: 1px dotted #ccc;
}
.orderBox .ul {
  background: #fff;
  border-radius: 10px;
  padding: 10px 0;
  color: #333;
}
.orderBox .ul .label {
  display: block;
}
.orderBox .ul .li {
  height: 30px;
  line-height: 30px;
  padding: 0 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.orderBox .ul .flexSpace {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.orderBox .ul .flexSpace .priceBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.orderBox .ul .flexSpace input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 10px;
  font-size: 14px;
}
.orderBox .ul .fontWeight {
  font-weight: bold;
}
.orderBox .dl {
  background: #FFFFFF;
  border-top: 10px solid #eee;
}
.orderBox .dl .dt {
  font-weight: bold;
  line-height: 30px;
  padding: 0 10px;
  font-size: 16px;
}
.orderBox .dl .dd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 10px;
  height: 30px;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}

