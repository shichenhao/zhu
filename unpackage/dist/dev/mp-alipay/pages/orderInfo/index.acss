.orderBox {
  min-height: 100vh;
  background: #eee;
}
.orderBox .result {
  position: absolute;
  padding-left: 10px;
  bottom: 0;
  left: 0;
  right: 0;
  height: 50px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 16px;
  background: #ffffff;
}
.orderBox .result .submit {
  padding: 0 20px;
}
.orderBox .price {
  color: #FFA200;
}
.orderBox .ulTitle {
  background: #eeeeee;
  line-height: 40px;
  padding: 0 10px;
  font-size: 16px;
}
.orderBox .ul {
  background: #fff;
  padding: 20px;
}
.orderBox .ul .label {
  display: block;
}
.orderBox .ul .li {
  padding: 10px 0px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.orderBox .ul .li .span {
  color: #333;
}
.orderBox .ul .li .title {
  width: 100px;
  padding-right: 0;
  text-align: right;
}
.orderBox .ul .flexSpace {
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.orderBox .ul .flexSpace .priceBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.orderBox .ul .flexSpace input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 30px;
  font-size: 14px;
}
.orderBox .ul .fontWeight {
  font-weight: bold;
}
.orderBox .dl {
  background: #FFFFFF;
}
.orderBox .dl .dt {
  background: #eeeeee;
  line-height: 40px;
  padding: 0 10px;
  font-size: 16px;
}
.orderBox .dl .dd {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 10px;
  height: 35px;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}

